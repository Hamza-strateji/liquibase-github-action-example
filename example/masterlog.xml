<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <includeAll path="changelogs/" relativeToChangelogFile="true" />
    <changeSet id="seed-sql" author="you">
        <sqlFile path="changesets/seed-data.sql" relativeToChangelogFile="false"/>
    </changeSet>
    <changeSet id="seed-data-user-001" author="you">
    <preConditions onFail="MARK_RAN">
        <not>
            <sqlCheck expectedResult="0">
                SELECT COUNT(*) FROM user WHERE UserName = 'admin'
            </sqlCheck>
        </not>
    </preConditions>

    <insert tableName="user">
        <column name="UserName" value="admin"/>
        <column name="Name" value="Aslan"/>
        <column name="Surname" value="Varol"/>
        <column name="created_at" value="2023-07-27 00:00:00+00"/>
    </insert>
</changeSet>
</databaseChangeLog>